	<div class="logporforgot">
	<form id="forgotForm" action="">
		<div class="forgotBox">
			<div class="Logforgot clearfix">
				<a href="/Derby/siteHome.do?p=index"><h3 class="logLogo"></h3></a>
				<p class="col-sm-12 forgotText">Enter your e-mail address, we will send a new password to you via e-mail.</p>
				<div class="col-sm-8 pB20"> 
					<div class="input-group">
						<div class="input-group-addon icon-contact"></div>
						<input type="email" value="Email"  name="email" id="txtemail"  class="form-control Inpfocus">
					</div>
				 </div>
				 <p class="col-sm-4"><button class="btn btn-Login" type="button" onclick="fsemail()">SUBMIT</button></p>
			</div>
		</div>
		</form>
	</div>
	  <script type="text/javascript">
	  	function forgotPassword(){
        		jQuery.ajax({
            type: "POST",
            url: "/Derby/siteHome.do?p=forgotPassword",
            data: $("#forgotForm").serialize(),
            async: false,
            success: function (data) {
                if (data.type == "SUCESSS") {
                 	  location.href = "sign-jump.html";
                } else
                    if (data.type == "ERROR") {
                        alert("Registration Error!");
                    } else {
                        alert(data.message);
                 	   }
         		  }
      			}) 
      			}
	  
	  
        function fsemail() {
            if ($("#txtemail").val() == "" || $("#txtemail").val() == "Email") {
                alert('Please enter the email address！');
                $('#txtemail').focus();
                return false;
            } else if (!Isyx($('#txtemail').val())) {
                alert('Please enter a valid email address！');
                $('#txtemail').focus();
                return false;
            }
            return forgotPassword();
        }
        function Isyx(yx) {
            var reyx = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+(.[a-zA-Z0-9_-])+/;
            return (reyx.test(yx));
        }
    </script> 
