	<div class="logporforgot">
	<form id="signForm" action="/Derby/siteHome.do?p=signLogin">
		<div class="forgotBox">
			<div class="Logforgot clearfix">
				<a href="index.html"><h3 class="logLogo"></h3></a>
				<div class="col-sm-6 pB20"> 
					<input type="text" value="* First Name" name="firstName" ID="txtname" class="form-control Inpfocus">
				 </div>
				<div class="col-sm-6 pB20"> 
					<input type="text" value="* Last Name" name="lastName" ID="txtusername" class="form-control Inpfocus">
				 </div>
        <div class="col-sm-6 pB20"> 
          <input type="text" value="* Password" class="form-control clickTextfocus">
		  <input type="password" value="" ID="txtpassword" name="password"  class="form-control clickfocus">
         </div>
        <div class="col-sm-6 pB20"> 
          <input type="text" value="* Confirm Password"  class="form-control clickTextfocus">
		  <input type="password" value="" ID="txtpassword1" class="form-control clickfocus">
         </div> 
				<div class="col-sm-6 pB20"> 
					<input type="text" value="* Email" ID="txtemail" name="email"  class="form-control Inpfocus">   
				 </div>
				<div class="col-sm-6 pB20"> 
					<input type="text" value="* Identity" name="identity"  ID="txtaddress"  class="form-control Inpfocus">   
					<!-- <select class="form-control">
					  <option>---Select Identity--- </option>
					  <option>2</option>
					  <option>3</option>
					  <option>4</option>
					  <option>5</option>
					</select>   -->
				</div>
				<div class="col-sm-12 pB20"> 
					<input type="text" value="* Company" name="company"  class="form-control Inpfocus">   
				 </div>
				<p class="col-sm-12"><button class="btn btn-Login" type="button" onclick="javascript:region();">SIGN UP</button></p>
			</div>
		</div>
		</form>
	</div>
<script type="text/javascript">
	function login(){
        		jQuery.ajax({
            type: "POST",
            url: "/Derby/siteHome.do?p=signLogin",
            data: $("#signForm").serialize(),
            async: false,
            success: function (data) {
                if (data.type == "SUCESSS") {
                 	  location.href = "sign-jump.html";
                } else
                    if (data.type == "ERROR") {
                        alert("Registration Error!");
                    } else {
                        alert(data.message);
                 	   }
         		  }
      			}) 
      			}
        function region() {
            var name = $('#txtname');
            var username = $('#txtusername');
            var pwd = $('#txtpassword');
            var pwd1 = $('#txtpassword1');
            var email = $('#txtemail');
            var address = $('#txtaddress');
            var company = $('#txtcompany');
            if (name.val() == "" || name.val() == "* First Name") {
                alert('Please enter your first name');
                name.focus();
                return false;
            } else if (username.val() == "" || username.val() == "* Last Name") {
                alert('Please enter your last name');
                username.focus();
                return false;
            } else if (pwd.val() == "") {
                alert('Please enter the password');
                $('#txtpwd').focus();
                pwd.focus();
                return false;
            } else if (pwd1.val() == "") {
                alert('Please enter the password confirmation');
                $('#txtpwd1').focus();
                pwd1.focus();
                return false;
            } else if (pwd.val() != pwd1.val()) {
                alert('The confirmation password is not correct');
                $('#txtpwd1').focus();
                pwd1.focus();
                return false;
            } else if (email.val() == "" || email.val() == "* Email") {
                alert('Please enter the mailbox');
                email.focus();
                return false;
            } else if (!Isyx(email.val())) {
                alert('Please enter a valid mailbox');
                email.focus();
                return false;
            } else if (address.val() == "" || address.val() == "* Identity") {
                alert('Please input the ID card');
                address.focus();
                return false;
            } else if (company.val() == "" || company.val() == "* Company") {
                alert('Please name of the Entering Firm');
                company.focus();
                return false;
            }
            return login();
        }
        function Isyx(yx) {
            var reyx = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+(.[a-zA-Z0-9_-])+/;
            return (reyx.test(yx));
        }
        
        
    </script>
